.PHONY: clean All

All:
	@echo "----------Building project:[ FirstProgram - Debug ]----------"
	@cd "C:\Code\CPP\FirstProgram" && "$(MAKE)" -f  "FirstProgram.mk"
clean:
	@echo "----------Cleaning project:[ FirstProgram - Debug ]----------"
	@cd "C:\Code\CPP\FirstProgram" && "$(MAKE)" -f  "FirstProgram.mk" clean
